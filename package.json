{
  "name": "@fena/woocommerce-rest-ts-api",
  "version": "1.0.1",
  "description": "WooCommerce REST API - Type Script Library",
  "author": "Yuri Lima",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/src/index.d.ts",
  "files": [
    "./dist/**/*"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/src/index.d.ts"
    }
  },
  "scripts": {
    "co:login": "aws codeartifact login --tool npm --repository fena --domain repo --domain-owner 578657107977",
    "co:publish": "npm publish",
    "build": "npm run prepack",
    "dep:update": "ncu && ncu --doctor -u && ncu -u",
    "test": "jest",
    "data:generate": "ts-node ./src/test/generate-data.ts",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "prepublishOnly": "npm run dep:update && npm run format",
    "clean": "rm -rf ./dist",
    "build:types": "npx tsc --emitDeclarationOnly --declaration",
    "build:cjs:esm": "npx tsup ./src/index.ts --format cjs,esm --sourcemap",
    "prepack": "npm run clean && npm run build:cjs:esm && npm run build:types",
    "semantic-release": "semantic-release",
    "commit": "cz",
    "postinstall": "tsc --outDir ./build"
  },
  "lint-staged": {
    "*.{ts,js,json}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "axios": "^1.7.2",
    "dynamic.envs": "^1.0.5",
    "oauth-1.0a": "^2.2.6",
    "typescript": "^5.4.5",
    "url-parse": "^1.5.10"
  },
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.0.6",
    "@types/jest": "^29.5.12",
    "@types/luxon": "^3.4.2",
    "@types/randomstring": "^1.3.0",
    "@types/url-parse": "^1.4.11",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "7.12.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "del-cli": "^5.1.0",
    "eslint": "9.4.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-n": "17.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.2.0",
    "jest": "^29.7.0",
    "lint-staged": "15.2.5",
    "luxon": "^3.4.4",
    "nock": "13.5.4",
    "npm-check-updates": "^16.14.20",
    "prettier": "3.3.1",
    "randomstring": "^1.3.0",
    "semantic-release": "^24.0.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "tsup": "^8.1.0"
  }
}
